{
  "manifest_version": 3,
  "name": "Formerly Twitter Text Remover",
  "version": "1.0.0",
  "description": "Removes annoying 'formerly Twitter' text from web pages",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "author": "Chris Reed"
}